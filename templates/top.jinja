              <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" href="#" id="navbarPastConferences" role="button" data-bs-toggle="dropdown" aria-expanded="false">Past Events</a>
                <ul class="dropdown-menu" aria-labelledby="navbarPastConferences">
                  {% for past_year in available_years %}
                  {% if past_year != 2026 %}
                  {% set past_rocc_year = "ROCC" + (past_year|string)[-2:] %}
                  <li><a class="dropdown-item" href="{{ past_year }}.html">{{ past_rocc_year }}</a></li>
                  {% endif %}
                  {% endfor %}
                </ul>
              </li>
              <li class="nav-item">
                {% if is_year_page %}
                <a class="nav-link" href="index.html">Home</a>
                {% else %}
                <a class="nav-link scrollto" href="#hero-block">Home</a>
                {% endif %}
              </li>
              <li class="nav-item">
                <a class="nav-link scrollto" href="#about-section">About</a>
              </li>
              {% if (is_post_event and testimonials and testimonials|length > 0) or (photo_highlights and photo_highlights|length > 0) %}
              <li class="nav-item">
                <a class="nav-link scrollto" href="#highlights-section">Highlights</a>
              </li>
              {% endif %}
              {% if has_speakers and (not is_year_page or is_post_event) %}
              <li class="nav-item">
                <a class="nav-link scrollto" href="#speakers-section">Speakers</a>
              </li>
              {% endif %}
              {% if has_schedules and (not is_year_page or is_post_event) %}
              <li class="nav-item">
                <a class="nav-link scrollto" href="#{{ schedules[0].id if schedules else 'schedule' }}">Schedule</a>
              </li>
              {% endif %}
              {% if tickets_on_sale and not is_post_event and not is_holding_page %}
              <li class="nav-item">
                <a class="nav-link scrollto" href="#tickets-section">Tickets</a>
              </li>
              {% endif %}
              {% if not is_post_event %}
              <li class="nav-item">
                <a class="nav-link scrollto" href="#venue-section">Venue</a>
              </li>
              {% endif %}
              {% if has_sponsors and (not is_year_page or is_post_event) %}
              <li class="nav-item">
                <a class="nav-link scrollto" href="#sponsors-section">Sponsors</a>
              </li>
              {% endif %}
              {% if has_exhibitors and (not is_year_page or is_post_event) %}
              <li class="nav-item">
                <a class="nav-link scrollto" href="#exhibitors-section">Exhibitors</a>
              </li>
